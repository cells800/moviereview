<% if !user_signed_in? %>  <div class="jumbotron">
    <h1> Your Favorite Movies Reviewed </h1>
    <p>Review Movie needs signin sadf sfsdf asdfsadf asfsadfsf asdfsafsdf asdfsadfa asdfasdfasdf asdfasfdasdf asdfsadfa </p>
    <p><%= link_to "Sign Up to Write A Review", new_user_registration_path, class: "btn btn-primary btn-lg" %></p>
  </div>
<% end %>

<div class="row">
  <% @movies.each do |movie| %>
    <div class="col-xs-6 col-md-3">
      <div class="thumbnail">
        <%= link_to (image_tag movie.image.url(:medium), class: 'image'), movie %>
      </div>
    </div>
  <% end %>
</div>
